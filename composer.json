{
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"craftcms/ckeditor": "^4.0.3",
		"craftcms/cms": "^5.0.0",
		"craftcms/google-cloud": "^2.2.0",
		"craftcms/mailgun": "^3.1.0",
		"mmikkel/cp-field-inspect": "^2.0.1",
		"nystudio107/craft-vite": "^5.0.0-beta.3",
		"putyourlightson/craft-elements-panel": "^3.0.0-beta.1",
		"putyourlightson/craft-sherlock": "^5.0.0-beta.3",
		"verbb/expanded-singles": "^3.0.0-beta.2",
		"verbb/image-resizer": "^4.0.0-beta.1",
		"vlucas/phpdotenv": "^5.4.0"
	},
	"require-dev": {
		"craftcms/generator": "^2.0.0",
		"yiisoft/yii2-shell": "^2.0.3"
	},
	"autoload": {
		"psr-4": {
			"modules\\": "modules/"
		}
	},
	"config": {
		"allow-plugins": {
			"craftcms/plugin-installer": true,
			"yiisoft/yii2-composer": true
		},
		"sort-packages": true,
		"optimize-autoloader": true,
		"platform": {
			"php": "8.2"
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		]
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.craftcms.com",
			"canonical": false
		}
	]
}
